gfx/atmospheric/testpoison01_256
{
	qer_editorimage	gfx/atmospheric/poison/testpoison01_256
	surfaceparm	trans
	q3map_onlyvertexlighting
	cull	twosided
    {
        map gfx/atmospheric/poison/testpoison01_256
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbGen vertex
    }
}

